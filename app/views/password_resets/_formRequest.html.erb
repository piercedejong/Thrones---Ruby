<%= form_for(:password_reset, url: password_resets_path) do |form| %>
  <form>
  <div class="form-group">
  <div class="input-group">
    <div class="input-group-prepend">
        <span class="input-group-text"> <i class="fa fa-user"></i> </span>
     </div>
     <%= form.text_field :email, placeholder:"Your Email", autocomplete:"new-password",class:"form-control input-group-form" %>
  </div> <!-- input-group.// -->
  </div> <!-- form-group// -->
  <div class="form-group">
  <div class="input-group">
    <div class="input-group-prepend">
        <span class="input-group-text"> <i class="fab fa-suse"></i> </span>
     </div>
     <%= form.text_field :username, placeholder:"Your Username", autocomplete:"new-password", class:"form-control input-group-form" %>
  </div> <!-- input-group.// -->
  </div> <!-- form-group// -->
  <div class="form-group">
  <%= form.submit "Reset Password", class:"btn btn-block btn-primary" %>
  </div> <!-- form-group// -->
  </form>
<% end %>
